pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building...'
                sh 'echo Building...'
            }
        }

        stage('Test') {
            steps {
                echo 'Running tests...'
                sh 'echo Running tests...'
            }
        }

        stage('Deploy') {
            steps {
                echo 'Deploying...'
                sh 'echo Deploying...'
            }
        }
    }
}
